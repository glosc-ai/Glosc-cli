{
    "name": "create-glosc",
    "version": "0.5.2",
    "description": "Scaffold Glosc projects (Python/TypeScript) via npm create",
    "author": "glosc-ai",
    "license": "MIT",
    "bin": {
        "create-glosc": "bin/index.js",
        "glosc": "bin/index.js"
    },
    "type": "commonjs",
    "engines": {
        "node": ">=18"
    },
    "scripts": {
        "build": "tsc -p tsconfig.json",
        "start": "npm run build && node bin/index.js",
        "lint": "npm run build --silent",
        "prepublishOnly": "npm run build"
    },
    "dependencies": {
        "prompts": "^2.4.2"
    },
    "devDependencies": {
        "@types/node": "^22.10.7",
        "@types/prompts": "^2.4.9",
        "typescript": "^5.4.5"
    },
    "files": [
        "bin/",
        "dist/",
        "src/",
        "README.md",
        "LICENSE"
    ],
    "keywords": [
        "create",
        "scaffold",
        "cli",
        "glosc"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/glosc-ai/Glosc-cli"
    }
}